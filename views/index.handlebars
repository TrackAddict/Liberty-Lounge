    {{#each news}}
    <div class="project" id="project{{id}}">
      <div class="thumbnail">
        <a href="#">
          <img src="{{image}}" alt="Lorem Pixel image" class="img project-img">
          <p>{{title}}</p>
        </a>
        <div class="details"></div>
      </div>
    </div>
    {{/each}}
      </div>

    <form id="new-project-form" role="form">
    <div class="form-group">
      <label for="title">Title for New Article:</label>
      <input type="text" class="form-control" id="title" placeholder="Project Name">
    </div>
    <div class="form-group">
      <label for="image_url">Article image URL</label>
      <input type="text" class="form-control" id="image_url" placeholder="http://www.example.com/really_cool_image.jpg">
    </div>
    <div class="form-group">
      <label for="date">Date</label>
      <input type="text" class="form-control" id="date" placeholder="January 1">
    </div>
    <div class="form-group">
      <label for="summary">Summary</label>
      <textarea type="text" class="form-control" id="summary" rows="3" placeholder="What does this article mean for Liberty?"></textarea>
    </div>
    <button id="newProjectSubmitButton" type="button" class="btn btn-default">Submit</button>
  </form>

